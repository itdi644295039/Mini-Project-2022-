    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        int love = 0 ;
         
        if (rdoMonday.isSelected()) {
            tarcharacter.setText("ปกติคนที่เกิดวันนี้เป็นคนที่ค่อนข้างแพรวพราว เป็นคนที่ค่อนข้างจะรักอิสระ ดูเป็นคนเจ้าชู้ไม่รู้ตัว เพราะเป็นคนมีเสน่ห์ วางตัวดี");
            tarlove.setText("ถ้ามีความรักขึ้นมาล่ะก็ เหมือนเป็นคนละคน! จากเสีอตัวใหญ่ พอมีแฟนแล้วจะกลายเป็นแมวน้อย ขี้อ้อน เอาใจ อยากได้อะไรจัดให้ทุกอย่าง งอแงกับแฟนเพราะอยากเรียกร้องความสนใจ ถ้าคบกันแล้วก็อยากให้ดูแล เอาอกเอาใจซึ่งกันและกัน มือถือนี่ถ้าไม่ล็อกรหัสก็เรียบร้อย! เพราะคนเกิดวันนี้จะแอบหึง แอบส่องทุกอย่าง แฟนไปไหน ทำอะไร อยู่กับใคร รู้หมด! แต่ คนที่เกิดวันจันทร์ถึงจะมีแฟนยังไงก็ยังไม่ทิ้งเพื่อน ถ้าอีกฝ่ายเข้าใจ ไม่คิดมากก็คบกันได้ยาว ๆ จ้า");
            love = 90;
        } else if(rdoTuesday.isSelected()) {
            tarcharacter.setText("คนเกิดวันนี้เป็นพ่อแม่บุญทุ่ม เวลารักใครจะจริงจังมากระดับสิบ! เป็นคนใจร้อน ตอบช้า 5 นาทีจะรู้สึกใจหายแล้ว ว่าเราพูดอะไรผิดไปไหมนะ? หรือว่าเขาไม่อยากคุยกับเราหรือเปล่า? หรือว่าเกิดเหตุร้ายอะไรขึ้น เอ๊ะ! หรือว่าแอบคุยกับคนอื่น");
            tarlove.setText("ถ้าอยู่ในช่วงอินเลิฟแล้วจะอารมณ์ดีทั้งวัน ยิ้มคนเดียว เป็นคนติดแฟน อยากอยู่ด้วยตลอดเวลา เธออยู่ที่ไหน จะไปอยู่ตรงหน้า จะไปหาแน่ ๆ! และค่อนข้างคาดหวังในตัวคนรักมาก ๆ ถ้าให้ 100 ต้องได้กลับมา 100 ชอบอะไรที่แฟร์ ๆ ถ้าไม่แฟร์ก็หาใหม่ ชอบความตื่นเต้นท้าทาย ถ้าอีกฝ่ายเล่นตัวนิดหน่อย เข้าหาบ้าง เอาใจก่อนบ้าง แกล้งไม่ตอบบ้าง ยิ่งตื่นเต้น ยิ่งถ้าอีกฝ่ายใส่ใจ และเข้าใจในตัวของคนที่เกิดวันอังคาร รับรองว่ารักจนมูฟออนไปไหนไม่ได้!");
            love = 100 ;
        }else if(rdoWednesday.isSelected()){
        tarcharacter.setText(" คารมดีเป็นที่ 1 ตบมุขโบ๊ะบ๊ะได้ ทันคน เทคนิคอ่อยชั้นครู แพรวพราวพอ ๆ กับคนเกิดวันจันทร์ เป็นคนที่ค่อย ๆ ดู ค่อย ๆ เก็บข้อมูลฝ่ายตรงข้าม ไม่วู่วาม ชอบนะ แต่ไม่แสดงออกตรง ๆ รอจนกว่าจะแน่ใจจริง ๆ แล้วถึงจะตกลงเป็นแฟน");
        tarlove.setText("เป็นคนที่รักใครแล้วรักจริง โรแมนติก เรื่องเล็ก ๆ น้อยก็จำได้ เอาใจใส่แฟนหรือคนรักมาก แต่ข้อเสียของคนที่เกิดวันนี้ เป็นเพราะอีโก้ที่สูงเสียดฟ้า อาจจะทำให้เรื่องราวที่มันไม่ได้มีอะไรเลย พังได้ง่าย ๆ เพราะความมีอีโก้ของตัวเอง เวลาโกรธมาก็จะใช้คำพูดที่ไม่ค่อยดี เป็นคนคาดหวังกับความรักสูง คิดมาก ขี้หึง ขี้ระแวง แล้วพอไม่ได้ตามที่หวัง ก็อาจจะพาลเบื่อได้ง่าย ๆ แนะนำให้ใจเย็น ๆ และลดความคาดหวังกับอีโก้ลงนิดหน่อย จะทำให้ความรักไปได้ดี");
        love = 50 ;
        
        }else if(rdoThursday.isSelected()){
        tarcharacter.setText("ภายนอกอาจจะดูเป็นคนอ่อยเก่ง แพรวพราว แต่จริง ๆ แล้วเป็นคนค่อนข้างอ่อนไหว เป็นคนออกจะบ๊อง ๆ ขาด ๆ เกิน ๆ ด้วยซ้ำ คนเกิดวันนี้ถึงจะดูอ่อยเก่งก็จริง ดูเป็นสาวฮอต หนุ่มฮอต แต่รักใครแล้วรักจริงจังมาก ไม่ชอบการนอกใจ และการถูกหักหลัง");
        tarlove.setText("เวลาคนเกิดวันพฤหัสบดีมีความรักแล้วเหมือนเมากัญชา อารมณ์ดีทั้งวัน และวันครบรอบ วันเกิด จำได้หมด ตกหลุมรักคนง่าย ยิ่งถ้าเจอคนที่คุยแบบ Deep Conversation ได้ ยิ่งชอบ ชอบคนที่เป็นผู้ใหญ่ ยิ่งมีทัศนคติดี ๆ หรือพลังบวก คนที่เกิดวันพฤหัสบดีจะมโนไปถึงขั้นแต่งงานเลย ทั้ง ๆ ที่แค่เริ่มคุย แต่ก็ชอบความรักที่ค่อยเป็นค่อยไป ไม่รีบนะ");
        lblLove.setText("60%");
        }else if(rdoFriday.isSelected()){
        tarcharacter.setText("คนเกิดวันนี้ออกแนวพ่อบ้านใจกล้า ไม่เถียงหรอกนะ แต่อย่าให้โกรธ! เป็นคนช่างมโน ชอบเพ้อเงียบ ๆ เป็นตุเป็นตะอยู่คนเดียว และคนเกิดวันนี้เป็นคนอ่อนโยน เอาใจใส่แฟนมาก ต้องการให้แฟนเอาใจใส่ตัวเองเหมือนกัน ถ้าเจอคนที่ทำให้คนเกิดวันศุกร์รู้สึกรักทั้งแฟน รักทั้งตัวเอง หรือชอบตัวเองตอนอยู่กับแฟน จะเป็นอะไรที่เพอร์เฟ็กต์มาก");
        tarlove.setText("ใครชอบคนสายเปย์ มาทางนี้! คนเกิดวันศุกร์บอกเลยว่าเปย์เก่งงงง ชอบหาอะไรทำ ชวนแฟนทำนู่น ทำนี่ อยู่ด้วยแล้วไม่มีทางเบื่อแน่นอน และมักจะเจอคนที่แตกต่างกันที่ อายุ เชื้อชาติ ศาสนา ฐานะ ฯลฯ แต่ไม่ค่อยสนใจเท่าไร ถ้าพร้อมจะเดินไปด้วยกันก็โอเค คบกันได้ยาว ๆ");
        love = 80;
        
        }else if(rdoSaturday.isSelected()){
        tarcharacter.setText("คนที่เกิดวันเสาร์ชอบทำตัวเป็นผู้นำ อีโก้สูง บ้าพลัง ไม่ชอบคนต่อล้อต่อเถียง ฉะนั้นคนที่นิ่ง ๆ สุขุมหน่อย ๆ ก็จะเข้าทางคนที่เกิดวันนี้ เป็นคนเด็ดขาด ชัดเจน ชอบก็คือชอบ ไม่ยึก ๆ ยัก ๆ ชอบคนเอาใจ ใส่ใจ ชอบอะไรที่เรียบง่าย มั่นคง ถ้ามีแฟนแล้วก็จะทุ่มสุด ");
        tarlove.setText("อยากได้อะไรบอกจะจัดให้ตามที่ขอทุกอย่าง ยกเว้นให้ลุงลาออก (เอ๊ย! ไม่ใช่!) ดูแลแฟนหรือคนรักดีเว่อร์! เหมือนเราเป็นเจ้าหญิงหรือเจ้าชาย แต่ไม่ชอบอะไรที่จุกจิก ชนิดว่าตามหึงตามจิกเกินเบอร์จะเบื่อมาก แต่ถึงจะไม่ชอบอะไรแบบนั้น คนที่เกิดวันเสาร์เวลาหึงจะน่ากลัวระดับสิบ!");
        love = 70;
        }else if(rdoSunday.isSelected()){
        tarcharacter.setText(" คนที่เกิดวันอาทิตย์ เขาว่าเป็นคนอารมณ์ขึ้น ๆ ลง ๆ อาจจะตามอารมณ์ยากสักหน่อย เป็นคนใจกว้าง ให้อิสระกับคนรัก แต่ถ้าเข้าโหมดจริงจังแล้วไม่ชอบอะไรเล่น ๆ อยู่ไหน ทำอะไรกับใคร ต้องบอก ต้องรายงาน และถ้าชอบใครก็จะทำทุกวิถีทางเพื่อให้ได้คบ");
        tarlove.setText("พอมีแฟนแล้วโลกสดใส อะไรก็เป็นสีชมพูไปหมด ถ้ามีแฟนจะขี้บ่นมาก ๆ แต่ก็เป็นเพราะรักแหละถึงบ่น เป็นคนรักเดียวใจเดียว คบใครก็คบทีละคน ไม่ได้หว่านไปทั่วถ้าเทียบกับคนเกิดวันอื่น ๆ ถ้าเจอแฟนหรือคนรักที่เข้ากับครอบครัวหรือเพื่อนได้ก็ผ่านฉลุย เพราะชอบความรักแบบลึกซึ้ง พร้อมที่จะเป็นครอบครัวเดียวกันในอนาคต ชอบคนพูดเพราะ ใส่ใจ ยิ้มหวาน ถ้าสอบผ่านก็จะเสมอต้นเสมอปลายกับคนรักมาก ๆ");
        love= 75 ;
        }
        lblLove.setText(love+"%");
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
             int option = JOptionPane.showConfirmDialog(rootPane, "Do u want to Exit");
        if (option == JOptionPane.YES_OPTION) {
            System.exit(0);

        }
    }                                        

